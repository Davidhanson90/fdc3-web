{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "name": "fdc3-web",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/js:tsc"
        },
        "generate-docs": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npx typedoc {projectRoot}/src/index.ts --out docs/{projectName} --options typedoc.json --tsconfig {projectRoot}/tsconfig.lib.json"
            }
        },
        "dependsOn": []
    }
}